    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1"
  },
  "lint-staged": {
    "*.{js,json}": [
      "./node_modules/.bin/prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ],
    "*.js": [
      "./node_modules/.bin/eslint --fix src/**/*.js",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e",
      "pre-commit": "./node_modules/.bin/git-authors-cli && ./node_modules/.bin/lint-staged"
    }
  },
  "contributors": [
